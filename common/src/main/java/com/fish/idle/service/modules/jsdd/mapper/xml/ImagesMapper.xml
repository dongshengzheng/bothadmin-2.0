<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fish.idle.service.modules.jsdd.mapper.ImagesMapper">

    <!-- 通用查询结果列-->
    <sql id="Base_Column_List">
		id, target_id AS targetId, url, `type`
	</sql>
    <!--根据作品id查询诠释图片-->
    <select id="selectByInterId" resultType="Images">
        SELECT * FROM dd_images i WHERE i.type='3' and i.target_id=#{target_id}
    </select>

</mapper>