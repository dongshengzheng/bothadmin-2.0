<mapper namespace="com.fish.idle.service.modules.jsdd.mapper.ProvinceMapper">

	<resultMap id="BaseResultMap" type="com.fish.idle.service.modules.jsdd.entity.Province">
		<id column="id" property="id" jdbcType="INTEGER" />
		<result column="provinceID" property="provinceid" jdbcType="VARCHAR" />
